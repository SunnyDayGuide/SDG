{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "adrianmejias/laravel-states": "^1.2",
        "cviebrock/eloquent-sluggable": "^4.6",
        "doctrine/dbal": "^2.8",
        "ebess/advanced-nova-media-library": "^2.0",
        "epartment/nova-dependency-container": "^1.1",
        "fideloper/proxy": "^4.0",
        "froala/nova-froala-field": "^1.1",
        "intervention/imagecache": "^2.3",
        "laravel/framework": "5.7.*",
        "laravel/horizon": "^2.0",
        "laravel/nova": "*",
        "laravel/telescope": "^1.0",
        "laravel/tinker": "^1.0",
        "predis/predis": "~1.0",
        "spatie/laravel-medialibrary": "^7.0.0",
        "spatie/laravel-tags": "^2.1",
        "spatie/nova-tags-field": "^1.1",
        "waynestate/nova-ckeditor4-field": "^0.2.0",
        "webpatser/laravel-countries": "dev-master"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": ["tests/utilities/functions.php"]
    },
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        }
    ],
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
